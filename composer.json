{
    "name": "watson/bootstrap-form",
    "description": "Laravel 5 form wrappers for Bootstrap 3.",
    "keywords": [
        "laravel",
        "form",
        "bootstrap"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dwight Watson",
            "email": "dwightwatson@me.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/PhuongDongIOT/laravel-collective-html"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/config": "^11.0",
        "illuminate/session": "^11.0",
        "illuminate/support": "^11.0",
        "laravelcollective/html": "dev-main"
    },
    "require-dev": {
        "mockery/mockery": "~1.4.1",
        "phpunit/phpunit": "~9.5"
    },
    "autoload": {
        "psr-4": {
            "Watson\\BootstrapForm\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Watson\\BootstrapForm\\BootstrapFormServiceProvider"
            ],
            "aliases": {
                "BootForm": "Watson\\BootstrapForm\\Facades\\BootstrapForm"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
